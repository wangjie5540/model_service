        cname: 客户号
        type: string
        required: true
      - name: trade_date
        cname: 交易日期
        type: date
        required: true
      - name: trade_type
        cname: 交易类型
        type: string
        required: true
      - name: fund_code
        cname: 基金代码
        type: string
        required: true
trainSample:
  - label: 事件选择
    itemList:
      - type: select
        attributeName: event_code_buy
        prefixText: 购买事件
        placeholder: 请选择购买事件
        description: 介绍
        required: true
        optionType: 1
        optionItems:
          - label: 购买
            value: fund_buy
  - label: 样本范围
    itemList:
      - type: select
        attributeName: sample_select__range
        prefixText: 过去
        placeholder: 请选择样本范围
        suffixText: 天的样本
        optionType: 1
        optionItems:
          - label: 30天
            value: 30
          - label: 60天
            value: 60
          - label: 90天
            value: 90
  - label: 样本比例
    itemList:
      - type: input
        attributeName: sample_select__pos_sample_proportion
        prefixText: 正样本比例
        suffixText: "%"
        pattern: "^1\\d{0,10}$"
        placeholder: 请输入正样本比例
        errorMessage: 请输入正确的样本比例
        required: true
        multiple: false
